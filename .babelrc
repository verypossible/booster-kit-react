{
  "presets": [["es2015", { "loose" : true }], "react", "stage-0"],
  "plugins": [
    "transform-runtime",
    "react-hot-loader/babel",
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    "transform-async-to-generator"
  ],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
