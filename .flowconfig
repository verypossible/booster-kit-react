[ignore]
.*/bin/.*
.*/blueprints/.*
.*/build/.*
.*/config/.*
.*/coverage/.*
.*/node_modules/.*
.*/src/styles/.*
.*/src/universal/styles/.*

[include]
./node_modules/

[libs]
./flow-interfaces/
./flow-typed/npm/

[options]
module.system=node
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.decorators=ignore

module.name_mapper='.*\.\(css\|scss\)$' -> 'css-module-flow'
module.name_mapper='.*\.\(jpg\|jpeg\|gif\|svg\|png\)$' -> 'Image'
module.name_mapper='^components\/\(.*\)$' -> '<PROJECT_ROOT>/universal/components/\1'
module.name_mapper='^components$' -> '<PROJECT_ROOT>/universal/components'
module.name_mapper='^features\/\(.*\)$' -> '<PROJECT_ROOT>/universal/features/\1'
module.name_mapper='^features$' -> '<PROJECT_ROOT>/universal/features'
module.name_mapper='^layouts\/\(.*\)$' -> '<PROJECT_ROOT>/universal/layouts/\1'
module.name_mapper='^layouts$' -> '<PROJECT_ROOT>/universal/layouts'
module.name_mapper='^modules\/\(.*\)$' -> '<PROJECT_ROOT>/universal/modules/\1'
module.name_mapper='^modules$' -> '<PROJECT_ROOT>/universal/modules'
module.name_mapper='^streams\/\(.*\)$' -> '<PROJECT_ROOT>/universal/streams/\1'
module.name_mapper='^streams$' -> '<PROJECT_ROOT>/universal/streams'
module.name_mapper='^store\/\(.*\)$' -> '<PROJECT_ROOT>/universal/store\1'
module.name_mapper='^router\/\(.*\)$' -> '<PROJECT_ROOT>/universal/router/\1'
module.name_mapper='^router$' -> '<PROJECT_ROOT>/universal/router'
module.name_mapper='^styles\/\(.*\)$' -> '<PROJECT_ROOT>/universal/styles/\1'
module.name_mapper='^styles$' -> '<PROJECT_ROOT>/universal/styles'
module.name_mapper='^lib\/\(.*\)$' -> '<PROJECT_ROOT>/universal/lib/\1'
module.name_mapper='^lib$' -> '<PROJECT_ROOT>/universal/lib'

munge_underscores=true

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe

suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe\\($\\|[^(]\\|(\\(>=0\\.\\(1[0-6]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*www[a-z,_]*\\)?)\\)
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue\\((\\(>=0\\.\\(1[0-6]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*www[a-z,_]*\\)?)\\)? #[0-9]+
suppress_comment=\\(.\\|\n\\)*\\$FlowFixedInNextDeploy
