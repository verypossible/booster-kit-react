{
  "name": "very-react",
  "version": "2.0.0",
  "homepage": "react.verypossible.com",
  "description": "Very's component library and react starter kit powered by React, Redux, React Router, and Styled Components",
  "main": "index.js",
  "private": "true",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "build": "bash -e scripts/build.sh",
    "clean": "yarn cache clean && yarn install --force",
    "clear": "rimraf build",
    "docs": "typedoc --options typedoc.js",
    "graphql": "bash -e scripts/graphql.sh",
    "init": "node scripts/init",
    "jest": "yarn run init && jest --config jest.config.json --colors",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "preflight": "yarn run lint && yarn run test",
    "start": "bash -e scripts/start.sh",
    "storybook:build": "build-storybook -c .storybook -0 .storybook-static",
    "storybook:start": "start-storybook -p 4003 -c .storybook",
    "test:watch": "bnr test --watch",
    "test": "bnr test",
    "test:ci": "bash -e scripts/runTests.sh",
    "test:features": "bnr test:features",
    "test:specs": "bnr test:specs"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/webpack.config -p --colors --progress --env.env=production",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "webpack-dev-server --config webpack/webpack.config --env.env=development",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "yarn run jest --runInBand",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*",
        "TEST_FEATURES": true,
        "TEST_SPECS": true,
        "COVERAGE": false
      }
    },
    "test:specs": {
      "command": "yarn run jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*",
        "TEST_SPECS": true
      }
    },
    "test:features": {
      "command": "yarn run jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*",
        "TEST_FEATURES": true
      }
    }
  },
