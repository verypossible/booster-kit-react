{
  "name": "very-react",
  "version": "2.0.0",
  "homepage": "react.verypossible.com",
  "description": "Very's component library and react starter kit powered by React, Redux, React Router, and Styled Components",
  "main": "index.js",
  "private": "true",
  "engines": {
    "node": ">=8.3.1"
  },
  "scripts": {
    "build": "bnr build",
    "clean": "yarn cache clean && yarn install --force",
    "clear": "rimraf build",
    "coverage": "CODECLIMATE_REPO_TOKEN=36bc060ce6f0aa2f739be4ae096f22356d8cf9fa6d15e5ca5ddfed318b5e6f85 codeclimate-test-reporter < coverage/lcov.info",
    "init": "node scripts/init",
    "jest": "yarn run init && jest --config jest.config.json --colors",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "preflight": "yarn run lint && yarn run test",
    "start": "bnr start",
    "storybook:build": "build-storybook -c .storybook -0 .storybook-static",
    "storybook:start": "start-storybook -p 4003 -c .storybook",
    "test:watch": "bnr test --watch",
    "test": "bnr test",
    "test:ci": "bash -e scripts/runTests.sh",
    "test:features": "bnr test:features",
    "test:specs": "bnr test:specs"
  },
  "betterScripts": {
    "build": {
      "command": "yarn run clear && yarn run init && webpack --config webpack/webpack.config --colors --progress --env.env=production",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "webpack-dev-server --config webpack/webpack.config --env.env=development",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "yarn run jest --runInBand",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*",
        "TEST_FEATURES": true,
        "TEST_SPECS": true,
        "COVERAGE": false
      }
    },
    "test:specs": {
      "command": "yarn run jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*",
        "TEST_SPECS": true
      }
    },
    "test:features": {
      "command": "yarn run jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*",
        "TEST_FEATURES": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spartansystems/booster-kit-react.git"
  },
  "author": "Spartan (http://joinspartan.com)",
  "license": "MIT",
