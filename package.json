{
  "name": "react-web-booster-kit",
  "version": "1.0.1",
  "homepage": "boosters.joinspartan.com",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "private": "true",
  "engines": {
    "node": ">=7.4.0"
  },
  "scripts": {
    "analyze": "flow",
    "build": "bnr build",
    "clean:project": "yarn cache clean && yarn install --force",
    "clean": "rimraf dist",
    "compile": "bnr compile",
    "debug:dev": "bnr debug:dev",
    "debug:prod": "bnr debug:prod",
    "deploy:prod": "bnr build && bnr publish:prod",
    "deploy:test": "bnr build && bnr publish:test",
    "dev": "bnr dev",
    "lint": "tslint bin build config server src tests",
    "preflight": "yarn run analyze && yarn run lint && yarn run test",
    "start": "bnr start",
    "storybook:build": "build-storybook -c .storybook -0 .storybook-static",
    "storybook:start": "start-storybook -p 4003 -c .storybook",
    "test:watch": "bnr test --watch --config tests/config/config.json",
    "test": "bnr test --config tests/config/config.json"
  },
  "betterScripts": {
    "compile": {
      "command": "node scripts/compile.babel",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec node scripts/server.babel",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "yarn run clean && yarn run compile",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "webpack-dev-server --config scripts/webpack.babel.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "publish:prod": {
      "command": "surge --project ./dist/ --domain boosters.joinspartan.com",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "publish:test": {
      "command": "surge --project ./dist/ --domain scary-coil.surge.sh",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "debug:dev": {
      "command": "node scripts/server.babel",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "*"
      }
    },
    "debug:prod": {
      "command": "node scripts/server.babel",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "*"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spartansystems/booster-kit-react.git"
  },
  "author": "Spartan (http://joinspartan.com)",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^15.0.33",
    "@types/react-dom": "^15.5.1",
    "@types/react-hot-loader": "^3.0.2",
    "@types/react-redux": "^4.4.45",
    "@types/react-router-dom": "^4.0.5",
    "@types/redux-form": "^6.6.4",
