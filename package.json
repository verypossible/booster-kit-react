{
  "name": "react-web-booster-kit",
  "version": "1.0.0-alpha.0",
  "homepage": "react.joinspartan.com",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "private": "true",
  "engines": {
    "node": ">=6.2.0",
    "npm": "^3.10.2"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start:express",
    "dev": "better-npm-run dev:static",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "publish": "better-npm-run publish",
    "publish:prod": "npm run deploy:prod && npm run publish",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "debug:dev": "better-npm-run debug:dev",
    "debug:prod": "better-npm-run debug:prod"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/express",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start:express": {
      "command": "babel-node bin/express",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "publish": {
      "command": "surge --project ./dist/ --domain boosters.joinspartan.com",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "debug:dev": {
      "command": "babel-node bin/express",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "*"
      }
    },
    "debug:prod": {
      "command": "babel-node bin/express",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spartansystems/booster-kit-react.git"
  },
  "author": "Spartan (http://joinspartan.com)",
  "license": "MIT",
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.11",
    "css-loader": "^0.25.0",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "npm": "^3.10.7",
    "phoenixjs": "^1.0.5",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-redux": "^4.0.0",
    "react-router": "^3.0.0-beta.1",
    "redux": "^3.6.0",
    "redux-form": "^6.0.5",
    "redux-socket-cluster": "^0.9.2",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.1",
    "shortid": "^2.2.6",
    "socketcluster-client": "^5.0.10",
    "style-loader": "^0.13.0",
    "sugarss": "^0.1.6",
    "url-loader": "^0.5.6",
    "webpack": "2.1.0-beta.22",
    "yargs": "^5.0.0"
  },
  "peerDependencies": {
    "cheerio": "^0.22.0",
    "socketcluster-client": "5.0.10"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^6.0.0-beta.6",
    "browser-sync": "^2.16.0",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.5.1",
    "cheerio": "^0.22.0",
    "codecov": "^1.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "enzyme": "^2.0.0",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-config-standard-react": "^4.0.2",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "isparta-loader": "^2.0.0",
    "karma": "^1.3.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "lost": "^7.1.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-font-magician": "^1.4.0",
    "postcss-reporter": "^1.3.3",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.2",
    "reactdown": "^1.0.0-alpha14",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "stylelint": "^7.3.1",
    "stylelint-config-standard": "^13.0.1",
    "surge": "^0.18.0",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.11.0"
  }
}
