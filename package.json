{
  "name": "react-web-booster-kit",
  "version": "1.0.0-alpha.0",
  "homepage": "boosters.joinspartan.com",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "private": "true",
  "engines": {
    "node": ">=7.4.0"
  },
  "scripts": {
    "analyze": "flow",
    "build": "bnr build",
    "clean:project": "yarn cache clean && yarn install --force",
    "clean": "rimraf dist",
    "compile": "bnr compile",
    "debug:dev": "bnr debug:dev",
    "debug:prod": "bnr debug:prod",
    "deploy:prod": "bnr build && bnr publish:prod",
    "deploy:test": "bnr build && bnr publish:test",
    "dev": "bnr dev",
    "flow-typed": "flow-typed install",
    "lint:fix": "yarn run lint -- --fix",
    "lint": "eslint bin build config server universal tests",
    "preflight": "yarn run analyze && yarn run lint && yarn run test",
    "start": "bnr start",
    "storybook:build": "build-storybook -c .storybook -0 .storybook-static",
    "storybook:start": "start-storybook -p 4003 -c .storybook",
    "test:watch": "bnr test --watch --config tests/config/config.json",
    "test": "bnr test --config tests/config/config.json"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile.babel",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec node bin/server.babel",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "yarn run clean && yarn run compile",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node bin/server.babel",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "publish:prod": {
      "command": "surge --project ./dist/ --domain boosters.joinspartan.com",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "publish:test": {
      "command": "surge --project ./dist/ --domain scary-coil.surge.sh",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "debug:dev": {
      "command": "node bin/server.babel",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "*"
      }
    },
    "debug:prod": {
      "command": "node bin/server.babel",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "*"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spartansystems/booster-kit-react.git"
  },
  "author": "Spartan (http://joinspartan.com)",
  "license": "MIT",
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
