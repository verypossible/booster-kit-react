    "es6-promise": "^4.1.1",
    "hoist-non-react-statics": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "phoenixjs": "^1.0.5",
    "react": "^15.6.1",
    "react-dom": "^15.3.2",
    "react-hot-loader": "next",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
