{
  "transform": {
      ".*": "<rootDir>/tests/config/preprocessor.js"
    },
  "globals": {
    "__DEV__": true,
    "__PROD__": false,
    "__DEBUG__": false,
    "__COVERAGE__": false,
    "__PROTOCOL__": false,
    "__HOST__": false,
    "__PORT__": false
  },
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json",
    "css",
    "sss",
    "scss",
    "md",
    "graphql"
  ],
  "moduleDirectories": [
    "node_modules",
    "universal",
    "client"
  ],
  "moduleNameMapper": {
    "^.+\\.(md|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
    "^.+\\.(css|less|scss)$": "identity-obj-proxy"
  },
  "testPathIgnorePatterns": [
    "(/blueprints/.*|\\.md$)"
  ]
}
